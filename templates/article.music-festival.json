    "f9e03650-258b-4f21-be30-118977b95a74": {
      "type": "image-with-text",
      "disabled": true,
      "settings": {
        "image": "shopify:\/\/shop_images\/11-04.jpg",
        "image_width": 700,
        "subheading": "",
        "title": "Gatsby Glamour Prom Dresses",
        "use_h1": false,
        "text": "<p>Gatsby Glamour Prom Dresses are the epitome of 1920s style. These dresses feature intricate beading, sequins, and fringe details that add a touch of glamour to your outfit. They come in a variety of colors, including classic black and gold, and can be worn in long or short lengths. Pair your Gatsby Glamour dress with a pair of strappy heels and a vintage-inspired clutch for a truly authentic look.<\/p>",
        "button_label": "",
        "button_link": "",
        "button_style": "primary",
        "image_position": "left",
        "text_alignment": "left",
        "use_alt_bg": false
      }
